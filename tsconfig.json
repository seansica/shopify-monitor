    "target": "ES2015",
    "module": "commonjs",
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": ["lambdas/**/*.ts", "lambdas/**/*.js"],
  "exclude": [
    "node_modules",
    "**/*.test.ts"
  ]
}
